generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int    @id @default(autoincrement())
  first_name      String
  last_name       String
  email           String
}

model App {
  id              String  @id @default(uuid())
  name            String
  description     String?
  certificate     String
  acs_url      String
  entity_id    String
}

